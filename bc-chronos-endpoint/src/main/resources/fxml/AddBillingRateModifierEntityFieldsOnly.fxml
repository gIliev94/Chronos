<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.Group?>

<GridPane gridLinesVisible="true" hgap="10" minHeight="200"
	minWidth="300" stylesheets="@../css/dark.css" vgap="10"
	AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="15.0"
	AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0"
	xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="bc.bg.tools.chronos.endpoint.ui.actions.entity.AddPerformerController">
	<columnConstraints>
		<ColumnConstraints hgrow="ALWAYS" percentWidth="40" />
		<ColumnConstraints hgrow="ALWAYS" percentWidth="60" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints percentHeight="25" vgrow="ALWAYS" />
		<RowConstraints percentHeight="25" vgrow="ALWAYS" />
		<RowConstraints percentHeight="25" vgrow="ALWAYS" />
		<RowConstraints percentHeight="25" vgrow="ALWAYS" />
	</rowConstraints>

	<children>
		<Label fx:id="lblName" prefWidth="100" maxWidth="200"
			text="%view.add.entity.performer.property.name.label" textAlignment="CENTER"
			GridPane.columnIndex="0" GridPane.columnSpan="1" GridPane.halignment="RIGHT"
			GridPane.rowIndex="0" />
		<TextField fx:id="txfFieldName" prefWidth="200" maxWidth="400"
			GridPane.columnIndex="1" GridPane.columnSpan="1" GridPane.halignment="LEFT"
			GridPane.rowIndex="0" />

		<Label fx:id="lblHandle" prefWidth="100" maxWidth="200"
			text="%view.add.entity.performer.property.handle.label"
			textAlignment="CENTER" GridPane.columnIndex="0" GridPane.columnSpan="1"
			GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
		<TextField fx:id="txfFieldHandle" prefWidth="200" maxWidth="400"
			GridPane.columnIndex="1" GridPane.columnSpan="1" GridPane.halignment="LEFT"
			GridPane.rowIndex="1" />

		<Label fx:id="lblPassword" prefWidth="100" maxWidth="200"
			text="%view.add.entity.performer.property.password.label"
			textAlignment="CENTER" GridPane.columnIndex="0" GridPane.columnSpan="1"
			GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
		<PasswordField fx:id="txfFieldPassword" prefWidth="200"
			maxWidth="400" GridPane.columnIndex="1" GridPane.columnSpan="1"
			GridPane.halignment="LEFT" GridPane.rowIndex="2" />

		<Label fx:id="lblEmail" prefWidth="100" maxWidth="200"
			text="%view.add.entity.performer.property.email.label" textAlignment="CENTER"
			GridPane.columnIndex="0" GridPane.columnSpan="1" GridPane.halignment="RIGHT"
			GridPane.rowIndex="3" />
		<TextField fx:id="txfFieldEmail" prefWidth="200" maxWidth="400"
			GridPane.columnIndex="1" GridPane.columnSpan="1" GridPane.halignment="LEFT"
			GridPane.rowIndex="3" />
	</children>
</GridPane>
